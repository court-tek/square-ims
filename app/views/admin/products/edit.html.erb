 <!-- page title area start -->
            <div class="d-none d-lg-block page-title-area">
                <div class="row align-items-center">
                    <div class="col-sm-6">
                        <div class="breadcrumbs-area clearfix">
                            <h4 class="page-title pull-left">Edit Product</h4>
                            <ul class="breadcrumbs pull-left">
                                <li><%= link_to "Dashboard", admin_dashboard_index_path %></li>
                                <li><%= link_to "All Products", admin_products_path %></li>
                                <li><span>Edit Product</span></li>
                            </ul>
                        </div>
                    </div>
                    <%= render "shared/admin-userprofile" %>
                </div>
            </div>
            <!-- page title area end -->
            <div class="main-content-inner">
                <div class="col-lg-6 col-ml-12">
                    <div class="row">
                        <!-- basic form start -->
                        <div class="col-12 mt-5">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="header-title">Basic form</h4>
                                        <%= form_with(url: admin_path, method: :post, local: true) do |form| %>
                                            <div class="form-group">
                                                <%= form.label :idempotency_key, for: "InputIdempotencyKey" %>
                                                <%= form.text_field :idempotency_key, value: "", readonly: true, class: "form-control", id:"InputInputIdempotencyKey", aria_describedby: "InputIdempotencyKeyHelp", placeholder: "Idempotency Key" %>
                                            </div>
                                            <div class="form-group">
                                                <%= form.label :price, for: "InputPriceAmount" %>
                                                <%= form.number_field :price_amount, value: "", class: "form-control", id: "example-number-input" %>
                                            </div>
                                            <%= form.submit "Edit Product", class: "btn btn-primary mt-4 pr-4 pl-4" %>
                                        <% end %>
                                    </div>
                                </div>
                        </div>
                        <!-- basic form end -->
                    </div>
                </div>
            </div>